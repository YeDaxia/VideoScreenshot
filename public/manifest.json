{
  "name": "Pintu Chrome Plugin",
  "version": "1.0",
  "description": "A Chrome extension to capture video frames and create a collage.",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "side_panel": {
    "default_path": "sidebar.html"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "sidePanel"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}